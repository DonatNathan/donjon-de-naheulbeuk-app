{
  "expo": {
    "name": "Naheulbeuk Dungeon",
    "slug": "naheulbeuk_dungeon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main.png",
    "scheme": "naheulbeukdungeon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E8E4D4",
        "foregroundImage": "./assets/images/main.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.donaaat.naheulbeuk_dungeon"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/main.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E8E4D4",
          "dark": {
            "backgroundColor": "#E8E4D4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "951f0113-72d5-4b45-9c6f-d0c5c29b45e2"
      }
    }
  }
}
